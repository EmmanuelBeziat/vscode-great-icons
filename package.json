{
	"name": "vscode-great-icons",
	"displayName": "VSCode Great Icons",
	"description": "A big pack of icons (200+) for your files.",
	"version": "3.0.0",
	"publisher": "emmanuelbeziat",
	"engines": {
		"vscode": "^1.70.0"
	},
	"scripts": {
		"build": "tsc && node ./dist/extension.js",
		"watch": "tsc -watch -p ./",
		"optimize:svg": "svgo -f ./icons",
		"optimize:png": "pngquant --quality=65-90 ./icons/*.png",
		"vsce:publish": "vsce publish",
		"vsce:patch": "vsce publish patch",
		"vsce:package": "vsce package"
	},
	"categories": [
		"Other",
		"Themes"
	],
	"contributes": {
		"iconThemes": [
			{
				"id": "vscode-great-icons",
				"label": "VSCode Great Icons",
				"path": "./icons.json"
			}
		]
	},
	"activationEvents": [
		"onStartupFinished"
	],
	"main": "./dist/extension.js",
	"icon": "images/vscode-great-icons_icon.png",
	"galleryBanner": {
		"color": "#e0e0e0",
		"theme": "light"
	},
	"keywords": [
		"icons",
		"theme",
		"icon-theme",
		"customization"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/EmmanuelBeziat/vscode-great-icons.git"
	},
	"author": {
		"email": "contact@emmanuelbeziat.com",
		"name": "Emmanuel BÃ©ziat",
		"url": "https://www.emmanuelbeziat.com"
	},
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/EmmanuelBeziat/vscode-great-icons/issues"
	},
	"homepage": "https://github.com/EmmanuelBeziat/vscode-great-icons#readme",
	"devDependencies": {
		"@types/node": "^22.1.0",
		"@types/vscode": "^1.92.0",
		"@vscode/vsce": "^2.31.1",
		"pngquant": "^4.2.0",
		"svgo": "^3.3.2",
		"typescript": "^5.5.4"
	}
}
